{
  "name": "logstash_pipeline",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_section_definition"
      }
    },
    "_section_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "input_definition"
        },
        {
          "type": "SYMBOL",
          "name": "filter_definition"
        },
        {
          "type": "SYMBOL",
          "name": "output_definition"
        }
      ]
    },
    "input_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "input"
        },
        {
          "type": "SYMBOL",
          "name": "section_block"
        }
      ]
    },
    "filter_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "filter"
        },
        {
          "type": "SYMBOL",
          "name": "section_block"
        }
      ]
    },
    "output_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "output"
        },
        {
          "type": "SYMBOL",
          "name": "section_block"
        }
      ]
    },
    "section_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "plugin_definition"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "plugin_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "plugin_block"
        }
      ]
    },
    "plugin_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "setting_statement"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "setting_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "bytes_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "hash"
        }
      ]
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "_number_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        }
      ]
    },
    "integer_literal": {
      "type": "PATTERN",
      "value": "[+-]?(0|[1-9](_?[0-9])*)"
    },
    "float_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[+-]?(0|[1-9](_?[0-9])*)"
          },
          {
            "type": "PATTERN",
            "value": "[.][0-9](_?[0-9])*"
          }
        ]
      }
    },
    "bytes_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[0-9](_?[0-9])*"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[\\s]?[KkMmGgTtPpEeZzYy][Ii]?[Bb]"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "[0-9](_?[0-9])*"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "[\\s]?[KkMmGgTtPpEeZzYy][Ii]?[Bb]"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          }
        ]
      }
    },
    "string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": ".*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          }
        ]
      }
    },
    "hash": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "hash_entry"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "hash_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hash_key"
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "SYMBOL",
          "name": "hash_value"
        }
      ]
    },
    "hash_key": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[a-z0-9-_]+"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "[a-z0-9-_]+"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          }
        ]
      }
    },
    "hash_value": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "(\\\\.|[^\\\"])*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "(\\\\.|[^\\'])*"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z-_]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "PATTERN",
      "value": "\\\\\\r?\\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

